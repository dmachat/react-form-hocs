{
  "name": "react-form-hocs",
  "version": "0.1.0",
  "description": "Higher order components for use building react forms",
  "main": "index.js",
  "engine": "node 6.9.1",
  "engines": {
    "node": "~6.9.1"
  },
  "scripts": {
    "test": "NODE_ENV=test npm run eslint && npm run jest",
    "jest": "jest --config .jestrc",
    "jest-update": "jest -u --config .jestrc --coverage",
    "eslint": "./node_modules/.bin/eslint --ext jsx --ext js -o reports/eslint-results.html -f html src",
    "dll": "node ./node_modules/webpack/bin/webpack.js --config webpack/config.dll.js",
    "predev": "npm run dll",
    "dev": "NODE_ENV=development node webpack-dev-server.js",
    "prebuild": "NODE_ENV=production npm run dll",
    "build": "NODE_ENV=production node ./node_modules/webpack/bin/webpack.js --config webpack/config.js --bail",
    "visualize": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js --config webpack/config.js --profile --json > stats.json",
    "prestart": "npm run build"
  },
  "author": "Drew Machat <dmachat@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "autobind-decorator": "1.3.3",
    "babel-core": "6.13.2",
    "babel-eslint": "6.1.2",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.4",
    "babel-plugin-istanbul": "2.0.0",
    "babel-plugin-transform-class-properties": "6.4.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-plugin-transform-runtime": "6.12.0",
    "babel-polyfill": "6.0.16",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "6.0.15",
    "babel-preset-stage-0": "6.5.0",
    "babel-runtime": "6.11.6",
    "colors": "1.1.2",
    "compression": "^1.6.0",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.23.1",
    "del": "2.1.0",
    "dotenv": "2.0.0",
    "eslint": "2.9.0",
    "eslint-config-airbnb": "9.0.1",
    "eslint-loader": "1.5.0",
    "eslint-plugin-import": "1.8.1",
    "eslint-plugin-jsx-a11y": "1.3.0",
    "eslint-plugin-react": "5.1.1",
    "estraverse-fb": "1.3.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.1",
    "file-loader": "0.9.0",
    "history": "^3.0.0",
    "html-webpack-plugin": "2.22.0",
    "identity-obj-proxy": "3.0.0",
    "image-webpack-loader": "2.0.0",
    "json-loader": "0.5.4",
    "node-notifier": "^4.4.0",
    "postcss-cssnext": "2.4.0",
    "postcss-import": "8.1.0",
    "postcss-loader": "0.10.0",
    "postcss-url": "5.0.2",
    "raw-loader": "0.5.1",
    "react": "15.4.2",
    "react-addons-shallow-compare": "15.4.2",
    "react-addons-test-utils": "15.4.2",
    "react-displace": "2.1.3",
    "react-dom": "15.4.2",
    "react-hot-loader": "3.0.0-beta.6",
    "react-redux": "5.0.2",
    "react-router": "2.7.0",
    "recompose": "0.22.0",
    "redux": "3.6.0",
    "style-loader": "0.13.1",
